        //\n              },\n            },\n          ],\n        });\n      } catch (error) {\n        let message = \"The request failed.\";\n        window.err = error;\n        console.log(error);\n        this.submitting = false;\n        if (error.response) {\n          message = error.response.data.message || message;\n        }\n        this.$q.notify({\n          progress: true,\n          message: message,\n          html: true,\n          // color: 'primary',\n          type: \"negative\",\n          timeout: 10000,\n          actions: [\n            {\n              label: \"Dismiss\",\n              color: \"white\",\n              handler: () => {\n                //\n              },\n            },\n          ],\n        });\n      }\n    },\n  },\n  computed: {\n    shortUserName() {\n      if (!this.$store.state.user) return \"\";\n\n      const name = this.$store.state.user.name;\n      let out;\n      if (name.length <= 9) {\n        out = name;\n      } else {\n        out = name.slice(0, 6) + \"...\";\n      }\n      return out;\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}