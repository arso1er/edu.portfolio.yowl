class=\"yl-title\">\n      <router-link to=\"/\">\n        <q-btn stretch flat color=\"white\" label=\"YOWL app\" />\n      </router-link>\n    </q-toolbar-title>\n    <div class=\"row\">\n      <q-btn\n        flat\n        round\n        dense\n        icon=\"search\"\n        class=\"q-mr-xs\"\n        @click=\"toggleSearch\"\n      />\n      <div v-if=\"$store.state.user\">\n        <q-btn-dropdown stretch flat>\n          <template v-slot:label>\n            <div class=\"row items-center no-wrap\">\n              <q-avatar size=\"24px\" class=\"q-mr-sm\">\n                <img\n                  :src=\"$store.state.env.apiBaseUrl + $store.state.user.picture\"\n                />\n              </q-avatar>\n              <div class=\"text-center text-capitalize\">{{ shortUserName }}</div>\n            </div>\n          </template>\n          <q-list>\n            <q-item clickable v-close-popup tabindex=\"0\">\n              <q-item-section>\n                <router-link to=\"/comments/add\">\n                  <q-item-label>Add comment</q-item-label>\n                </router-link>\n              </q-item-section>\n            </q-item>\n            <q-item clickable v-close-popup tabindex=\"0\">\n              <q-item-section>\n                <router-link\n                  :to=\"{\n                    name: 'UserComments',\n                    params: { id: $store.state.user.id },\n                  }\"\n                >\n                  <q-item-label>My comments</q-item-label>\n                </router-link>\n              </q-item-section>\n            </q-item>\n            <q-item clickable v-close-popup tabindex=\"0\">\n              <q-item-section>\n                <router-link to=\"/my-profile\">\n                  <q-item-label>My profile</q-item-label>\n                </router-link>\n              </q-item-section>\n            </q-item>\n            <q-item\n              clickable\n              v-close-popup\n              tabindex=\"0\"\n              v-if=\"$store.state.user.role === 'admin'\"\n            >\n              <q-item-section>\n                <router-link to=\"/dashboard/admin\">\n                  <q-item-label>Admin dashboard</q-item-label>\n                </router-link>\n              </q-item-section>\n            </q-item>\n            <q-item clickable v-close-popup tabindex=\"0\" @click=\"logout\">\n              <q-item-section>\n                <q-item-label>Log out</q-item-label>\n              </q-item-section>\n            </q-item>\n          </q-list>\n        </q-btn-dropdown>\n      </div>\n      <div v-else>\n        <router-link to=\"/register\">\n          <q-btn stretch flat color=\"white\" label=\"Register\" />\n        </router-link>\n        <router-link to=\"/login\">\n          <q-btn stretch flat color=\"white\" label=\"Login\" />\n        </router-link>\n      </div>\n    </div>\n  </div>\n  <div v-else class=\"xs row items-center justify-between col\">\n    <q-btn flat round dense icon=\"west\" class=\"q-mr-sm\" @click=\"toggleSearch\" />\n    <HeaderSearch autofocus />\n  </div>\n</template>\n\n<style lang=\"scss\" scoped>\n.yl-title {\n  flex: 0 0 auto;\n}\n</style>\n\n<script>\nimport HeaderSearch from \"./HeaderSearch.vue\";\n\nexport default {\n  name: \"HeaderMobile\",\n  props: {\n    shortUserName: {\n      type: String,\n      default: () => \"\",\n    },\n  },\n  components: {\n    HeaderSearch,\n  },\n  data() {\n    return {\n      text: \"\",\n      searchView: false,\n    };\n  },\n  methods: {\n    toggleSearch() {\n      this.searchView = !this.searchView;\n    },\n    logout() {\n      this.$emit(\"logout\");\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}